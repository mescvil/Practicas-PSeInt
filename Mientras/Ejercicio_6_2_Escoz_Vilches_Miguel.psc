Proceso Ejercicios_6
	Definir a,b Como Entero;
	a <- -1;
	b <- -1;
	
	Mientras a <> 0 & b <> 0 Hacer
	Escribir "Introduce un numero ";
	Leer a;
	Escribir "Introduce un otro numero ";
	Leer b;
	Si a <> 0 & b <> 0 Entonces
		Escribir  a," + ",b," = ", a+b;
	FinSi
FinMientras
	Escribir "Fin de la ejecución";
FinProceso
