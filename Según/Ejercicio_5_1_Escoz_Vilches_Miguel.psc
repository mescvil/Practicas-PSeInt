Proceso Ejercicio_5_1
	Definir dia Como Entero;
	Escribir  Sin Saltar "Introduce un número del día de la semana (1 a 7) ";
	Leer dia;
	
	Segun dia Hacer
		1:
			Escribir "Lunes";
		2:
			Escribir "Martes";
		3:
			Escribir "Miercoles";
		4:
			Escribir "Jueves";
		5:
			Escribir "Viernes";
		6:
			Escribir "Sabado";
		7:
			Escribir "Domingo";
	FinSegun
	
FinProceso
